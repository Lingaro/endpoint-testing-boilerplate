{
  "name": "enpoint-testing",
  "version": "1.0.0",
  "license": "UNLICENCED",
  "scripts": {
    "test": "cross-env URL=https://jsonplaceholder.typicode.com jest --ci",
    "start": "cross-env URL=https://jsonplaceholder.typicode.com jest --watch --notify",
    "lint": "tslint 'src/**/*.ts' --format stylish"
  },
  "dependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.27",
    "@types/rest": "^1.3.28",
    "cross-env": "^5.0.5",
    "jest": "^21.0.1",
    "rest": "^2.0.0",
    "ts-jest": "^21.0.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testURL": "see: https://github.com/cujojs/rest/issues/135"
  }
}
